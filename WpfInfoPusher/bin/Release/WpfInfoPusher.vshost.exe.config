<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <!--显示格式配置，\r\n-回车换行，{0}-车牌号，{1}-时间-->
    <add key="format" value=" 停车一分钟\r\n    记点\r\n {0}\r\n  {1}" />
    <!--报文格式配置，{报文类型@设备编号@车来报文识别号@到达时间@列车去向@辆数_车型_车号_配属段}-->
    <add key="msgformat" value="1@0@{0}@{1}@{2}@1_{3}_{4}_0224" />
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>
</configuration>
